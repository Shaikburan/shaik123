#include<iostream>
using namespace std;
class student
{
	public:
		int no,t1,t2,result;
		void getdata()
		{
			cout<<"enter the regno "<<endl;
			cin>>no;
		}
};
class test:public student
{
	public:
		void getdata1()
		{
			cout<<"enter the  two test marks "<<endl;
			cin>>t1>>t2;
			cout<<"the result of student "<<t1+t2;
		}
};
int main()
{
	test m;
	m.getdata();
	m.getdata1();
}
